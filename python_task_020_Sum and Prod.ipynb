    "import numpy\n",
    "\n",
    "N, M = map(int, input().split())\n",
    "\n",
    "array = numpy.array([list(map(int, input().split())) for _ in range(N)])\n",
    "sum_axis = numpy.sum(array, axis=0)\n",
    "result = numpy.prod(sum_axis)\n",
    "\n",
    "print(result)"
